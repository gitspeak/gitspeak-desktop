{
  "name": "gitspeak",
  "version": "0.0.2",
  "productName": "GitSpeak",
  "description": "Desktop client for GitSpeak",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start-dev": "open node_modules/electron/dist/Electron.app --args $PWD",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "watch": "imbac -w -o lib src",
    "postinstall": "electron-builder install-app-deps",
    "gh-publish": "electron-builder --mac -p always"
  },
  "repository": "https://gitspeak.com",
  "keywords": [
    "git",
    "gitspeak",
    "github",
    "pr",
    "liveshare",
    "commit"
  ],
  "author": "Scrimba AS",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "electron-packager": "^12.2.0"
  },
  "build": {
    "appId": "com.gitspeak.app",
    "asar": false,
    "publish": {
      "provider": "github",
      "repo": "gitspeak-desktop",
      "owner": "scrimba"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icons/mac/icon.icns",
      "category": "your.app.category.type"
    },
    "win": {
      "target": "NSIS",
      "icon": "assets/icons/win/icon.icns"
    }
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.3",
    "find-free-port": "^2.0.0",
    "fix-path": "^2.1.0",
    "git-repo-info": "^2.0.0",
    "hosted-git-info": "^2.7.1",
    "imba": "^1.3.3",
    "install": "^0.12.2",
    "isbinaryfile": "^3.0.3",
    "msgpack-lite": "^0.1.26",
    "node-pty": "^0.7.8",
    "node-watch": "^0.5.8",
    "npm": "^6.4.1",
    "parse-git-config": "^2.0.3",
    "simple-git": "^1.106.0",
    "ws": "^6.1.0"
  }
}
